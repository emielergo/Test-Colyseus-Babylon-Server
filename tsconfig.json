{
    "compilerOptions": {
        "outDir": "lib",
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "allowJs": true,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "types": ["babylonjs", "babylonjs-loaders", "babylonjs-gui", "babylonjs-materials", "node"],
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
}
